        "build-client-dev": "webpack --watch --mode development --devtool inline-source-map --config webpack.common.js",
        "build-client-prod": "webpack --mode production --config webpack.common.js",
        "run-postbuild": "node postbuild.js",
        "run-server": "nodemon -r dotenv/config server.js",
        "dev": "yarn build-lib-dev && concurrently \"yarn run-server\" \"yarn build-client-dev\"",
        "build": "yarn build-lib-prod && yarn build-client-prod && yarn run-postbuild",
        "start-with-env": "node -r dotenv/config server.js",
